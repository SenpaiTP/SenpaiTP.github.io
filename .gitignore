# Basic Jekyll gitignores (synchronize to Jekyll.gitignore)
_site/
.sass-cache/
.jekyll-cache/
.jekyll-metadata

# Additional Ruby/bundler ignore for when you run: bundle install
/vendor

# Specific ignore for GitHub Pages
Gemfile.lock

# Node.js and React-specific ignores
node_modules/      # Dependencies installed by npm or yarn
npm-debug.log*     # Logs from npm
yarn-debug.log*    # Logs from Yarn
yarn-error.log*    # Error logs from Yarn
package-lock.json* # Lock file for npm (if you don't want to track it)
.env               # Environment variable files

# Build output directory (React default)
build/             # Production build files (if deploying from this folder)

# IDE-specific ignores
.vscode/           # VS Code settings
.idea/             # IntelliJ IDEA settings
*.iml              # IntelliJ IDEA module files

# OS-specific ignores
.DS_Store          # macOS file
Thumbs.db          # Windows file
